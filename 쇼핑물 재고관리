#include <stdio.h>

int main()
{
    int num,id;
    int ip[101]={};
    int pm[101]={};
    
    scanf("%d", &num);
    
    for(int i=0; i<num; i++ ){
        scanf("%d", &ip[i]);
    }
    for(int i=0; i<num; i++ ){
        scanf("%d", &pm[i]);
    }
    scanf("%d", &id);
    printf("%d\n", ip[id-1]-pm[id-1]);
    for(int i=0; i<num; i++ ){
        printf("%d ", ip[i]-pm[i]);
    }

    return 0;
}
